{
  "env": {
    "browser": true,
    "mocha": true,
    "jest": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "rules": {
    "no-param-reassign": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used", "argsIgnorePattern": "^_"}],
    "no-console": 0
  }
}
